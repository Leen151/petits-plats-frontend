{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "node": true,
        "browser": true,
        "document": true
    },
    "rules": {
        "no-unused-vars": "warn", //"off" pour l'annuler
        "no-undef": "warn",
        "linebreak-style": 0,
        "implicit-arrow-linebrek": 0,
        "function-paren-newline": 0,
        "indent": ["error", "tab"],
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-console": "warn",
        "no-var": "error"
    }
}